{
    "version": "0.0.1",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/out/main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "prestart",
            "runtimeExecutable": "${workspaceRoot}/node_modules/electron/dist/electron.exe",
            "runtimeArgs": [
                "."
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": true,
            "outDir": null
        },
        {
            "name": "Attach to renderer",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true
        }
    ]
}