{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "eslint.enable": true,
  "eslint.run": "onSave",
  "jest.pathToJest": "node_modules\\.bin\\jest",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "TodoParser": {
    "folderExclude": [
      "node_modules",
      ".vscode",
      "typings"
    ]
  },
  "files.associations": {
    "*.ksy": "yaml",
    "array": "cpp",
    "bitset": "cpp",
    "deque": "cpp",
    "initializer_list": "cpp",
