{
  "name": "gamebryo-plugin-management",
  "version": "0.0.1",
  "description": "Management for gamebryo plugins",
  "main": "./out/index.js",
  "repository": "",
  "scripts": {
    "webpack": "webpack --config webpack.config.js --display-error-details --progress --profile --color",
    "postinstall": "electron-rebuild -w esptk,loot -t dev,prod",
    "copy_esptk": "copyfiles -f ./node_modules/esptk/build/Release/nbind.node ./dist/esptk",
    "copy_loot": "copyfiles -f ./node_modules/loot/build/Release/nbind.node ./dist/loot && copyfiles -f ./node_modules/loot/loot_api/loot_api.dll ./dist/loot",
    "prebuild": "typings prune && typings install",
    "build": "npm run webpack && npm run copy_esptk && npm run copy_loot && copyfiles -f src/stylesheets/plugin_management.scss dist && extractInfo",
    "installDev": "copyfiles -u 1 dist/**/* ../../out/bundledPlugins/gamebryo-plugin-management",
    "buildDev": "npm run build && npm run installDev"
  },
  "author": "Black Tree Gaming Ltd.",
  "license": "GPL-3.0",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "@types/iconv-lite": "^0.0.1",
    "@types/immutability-helper" : "^2.0.15",
    "@types/react": "^16.0.3",
    "@types/react-bootstrap": "^0.0.52",
    "@types/react-dom": "^15.5.4",
    "@types/react-i18next": "^1.7.5",
    "@types/react-redux": "4.4.35",
    "@types/react-router": "^2.0.41",
    "@types/react-select": "1.0.47",
    "@types/js-yaml": "^3.5.29",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "bluebird": "^3.4.6",
    "copyfiles": "^1.0.0",
    "electron-rebuild": "^1.4.0",
    "esptk": "file:../../lib/cpp/esptk",
    "fs-extra-promise": "^0.4.1",
    "i18next": "^8.4.3",
    "iconv-lite": "^0.4.15",
    "immutability-helper": "^2.2.2",
    "json-loader": "^0.5.4",
    "loot": "file:../../lib/cpp/loot",
    "vortex-api": "file:../../api",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.3.2",
    "react-i18next": "^1.10.1",
    "react-select": "^1.0.0-rc.5",
    "redux-act": "^1.1.0",
    "webpack": "^3.5.5",
    "js-yaml": "^3.8.2",
    "ts-loader": "2.3.4"
  }
}
