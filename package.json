{
  "private": true,
  "name": "vortex_devel",
  "version": "0.0.1",
  "main": "out/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nexus-Mods/Vortex-Private"
  },
  "scripts": {
    "_assets_out": "node InstallAssets.js out",
    "_assets_app": "node InstallAssets.js app",
    "_update_submodules": "git submodule foreach git pull origin master || exit /b 0",
    "_install_app": "cd app && rm-local-modules && yarn install --non-interactive --check-files && yarn upgrade",
    "subprojects": "yarn run _update_submodules && node BuildSubprojects.js out",
    "subprojects_app": "yarn run _update_submodules && node BuildSubprojects.js app",
    "build_api": "cd api && yarn run build && yarn link && cd .. && yarn link vortex-api",
    "build_rest": "yarn run _assets_out && yarn run build_api && yarn run subprojects",
    "predist": "yarn run --non-interactive build_api && yarn run --non-interactive _install_app && yarn run --non-interactive subprojects_app && yarn run --non-interactive _assets_app",
    "dist": "electron-builder",
    "start": "cross-env NODE_ENV=development electron .",
    "clean": "yarn add rm-local-modules && rm-local-modules && yarn install --check-files",
    "preinstall": "git submodule update --init --recursive",
    "test": "jest --no-cache",
    "doc": "typedoc --out doc src",
    "build": "yarn run build_rest && tsc -p .",
    "buildwatch": "yarn run build_rest && tsc -p . -w"
  },
  "engines": {
    "electron": "1.7.9"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx|ts|tsx)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/app/"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "collectCoverage": true
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "7z.exe",
      "LICENSE.md",
      "bundledPlugins",
      "lib/modinstaller",
      "node_modules/7z-bin",
      "node_modules/bootstrap-sass/assets/stylesheets",
      "node_modules/react-select/scss",
      "assets/css/**"
    ],
    "appId": "com.nexusmods.vortex",
    "win": {
      "target": "nsis",
      "publish": [
        {
          "provider": "github",
          "token": "008e0c7bfa1a9a2f14df6370dfdac9340ec828a",
          "private": true
        }
      ]
    },
    "nsis": {
      "perMachine": true,
      "runAfterFinish": true,
      "menuCategory": true
    },
    "extraResources": [
      "locales"
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin${/*}",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/*.js.map",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "!**/*.{pdb,lib,VC.db,exp,gypi,d.ts}",
      "!**/build/Debug${/*}",
      "!**/build/Release/obj${/*}",
      "!**/build/.vs${/*}",
      "!**/autogypi.json",
      "!**/binding.gyp",
      "!**/binding.map",
      "!**/build/src/Release/obj${/*}",
      "!**/node_modules/edge/lib/native${/*}",
      "!**/node_modules/edge/src${/*}",
      "!**/node_modules/leveldown/deps${/*}",
      "!**/node_modules/node-sass/{build,src,vendor}${/*}",
      "!**/node_modules/react-layout-pane/{demo,docs}${/*}",
      "!**/node_modules/recharts/umd${/*}",
      "!**/node_modules/ffi/deps",
      "!**/Release/*.map",
      "!**/node_modules/bootstrap-sass/assets/fonts",
      "!**/node_modules/recharts/node_modules/d3-scale/img",
      "!**/node_modules/recharts/node_modules/d3-shape/img",
      "!**/node_modules/d3-geo/img"
    ]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
