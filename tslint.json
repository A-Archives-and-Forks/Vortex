{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "max-line-length": [
      true,
      100
    ],
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "jsx-no-lambda": true,
    "jsx-alignment": true,
    "jsx-no-multiline-js":  false
  },
  "rulesDirectory": []
}